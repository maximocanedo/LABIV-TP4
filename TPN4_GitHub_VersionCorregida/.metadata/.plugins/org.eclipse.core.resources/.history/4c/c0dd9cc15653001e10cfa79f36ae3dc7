package menu;

public class Validaciones {
	
	public boolean verificarNumero(String cadenaNumero) {
		boolean NUMERO_VALIDO = true;
		
		try {
			if(Float.parseFloat(cadenaNumero) <= 1.0f || Float.parseFloat(cadenaNumero) > 10.0f) {
				NUMERO_VALIDO=false;
			}
		}catch(Exception e) {
			NUMERO_VALIDO = false;
		}			
		
		return NUMERO_VALIDO;
	}
	
	
	
	
}
